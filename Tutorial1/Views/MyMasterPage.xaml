                <Button FontSize="20" Text="Item1" Clicked="btnClicked"></Button>
                <Button FontSize="20" Text="Item2" Clicked="btnTabbedClicked"></Button>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>